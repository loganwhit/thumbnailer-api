{
  "version": "2.0",
  "app_name": "thumbnailer-api",
  "manage_iam_role": true,
  "autogen_policy": false,
  "environment_variables": {
    "CACHE_BUCKET": ""
  },
  "lambda_memory_size": 128,
  "lambda_timeout": 30,
  "tags": {
    "Project": "photos"
  },
  "stages": {
    "dev": {
      "api_gateway_stage": "dev",
      "iam_policy_file": "policy-dev.json",
      "environment_variables": {
        "DEBUG": "1",
        "CONTENT_AGE_IN_SECONDS": "60"
      },
      "tags": {
        "Stage": "dev"
      }
    },
    "prod": {
      "api_gateway_stage": "prod",
      "iam_policy_file": "policy-dev.json",
      "lambda_memory_size": 256,
      "environment_variables": {
        "CONTENT_AGE_IN_SECONDS": "3600"
      },
      "tags": {
        "Stage": "prod"
      }
    }
  }
}